.vTitle {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #fafafa;
}

.tLabel {
  margin-top: 90rpx;
  font-size: 40rpx;
  height: 56rpx;
}

.tMessage {
  height: 84rpx;
  font-size: 28rpx;
  line-height: 3;
  text-align: center;
  color: #888;
}

.vTitle image {
  margin-top: 200rpx;
  height: 300rpx;
}

.bBack {
  width: 90%;
  height: 94rpx;
  line-height: 94rpx;
  border: solid 4rpx rgba(5, 5, 5, 0.1);
  margin-top: 112rpx;
  font-size: 36rpx;
  text-align: center;
  background-color: #1976d2;
  color: #fff;
}

.btn-hover {
  background-color: #1976d2;
  opacity: 0.7;
}
